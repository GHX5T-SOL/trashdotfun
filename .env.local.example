# Storacha Network Configuration (UCAN-based IPFS)
# Get these values by running: node scripts/setup-storacha.js

# Your Storacha DID key (from: storacha key create --json)
NEXT_PUBLIC_STORACHA_DID_KEY=did:key:z6Mk...

# Your UCAN proof (from: storacha delegation create [DID] -c space/blob/add -c space/index/add -c filecoin/offer -c upload/add --base64)
NEXT_PUBLIC_STORACHA_UCAN_PROOF=mAYIEAP8OEaJlcm9vdHOA...

# Gorbagana Chain Configuration
NEXT_PUBLIC_RPC_ENDPOINT=https://rpc.gorbagana.wtf/
NEXT_PUBLIC_EXPLORER_URL=https://trashscan.io

# Optional: Custom IPFS Gateway (defaults to ipfs.io)
NEXT_PUBLIC_IPFS_GATEWAY=https://ipfs.io

# Optional: Enable debug logging
NEXT_PUBLIC_DEBUG_IPFS=true
